#! /usr/bin/env python
# encoding: utf-8

def build(bld):
	bld.shlib(include="include",  target = 'test-serialize-modules', source = 'test-serialize-modules.cpp' )

	bld.program(include="include",  features='test cxx cxxprogram', use = "AutoConfigurator test-serialize-modules" ,  target = 'fileconfiguration-test',	source = 'fileconfiguration-test.cpp', install_path = None,)

# -*- indent-tabs-mode: t -*-
