#! /usr/bin/env python
# encoding: utf-8

def build(bld):
    bld.shlib(includes="",  use = "BOOST siox-core-ContainerSerializer siox-core-AutoConfigurator ", target = 'siox-monitoring-activityPlugin-ActivityFileWriter', source = 'ActivityFileWriter.cpp ActivityFileWriterOptions.cpp' )

    bld.recurse(['test'], mandatory=True)


