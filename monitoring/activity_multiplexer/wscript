#! /usr/bin/env python
# encoding: utf-8

def build(bld):

    bld.shlib(includes=["#/monitoring"], use="BOOST siox-core-ContainerSerializer siox-core-AutoConfigurator siox-core-AutoRegistrar", target='siox-monitoring-ActivityMultiplexerAsync', source='ActivityMultiplexerAsync.cpp',  )

    bld.recurse(['plugins'], mandatory=True)


